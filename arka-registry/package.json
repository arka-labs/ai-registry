{
  "name": "arka-registry",
  "version": "0.0.1",
  "description": "Arka Registry",
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
