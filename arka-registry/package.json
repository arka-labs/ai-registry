    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "deepmerge": "^4.2.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@types/node": "^22.10.2"
  }
