{
  "name": "arka-ai-registry",
  "version": "0.0.3",
  "description": "Arka AI Registry",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/types/index.js",
      "import": "./dist/types/index.mjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist/**",
    "codegen": "ts-node ./scripts/build.ts",
    "build": "npm run codegen; npm run clean; tsup"
  },
  "keywords": [
    "arka-registry",
    "ai-registry",
    "ai-agents",
    "interchain-ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
